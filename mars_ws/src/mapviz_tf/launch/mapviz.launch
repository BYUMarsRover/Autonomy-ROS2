xml
<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <set_env name="ROSCONSOLE_FORMAT" value="[${thread}] [${node}/${function}:${line}]: ${message}"/>

  <arg name="print_profile_data" default="false"/>

  <arg name="location" default="$(optenv MAPVIZ_LOCATION hanksville)"/>

  <node pkg="mapviz" exec="mapviz" name="mapviz">
    <param name="print_profile_data" value="$(arg print_profile_data)"/>
    <param name="config" value="$(env HOME)/BYU-Mars-Rover/rover_ws/src/mapviz_tf/scripts/.mapviz_config"/>
  </node>

  <group if="$(eval location == 'byu')">
    <param name="map_origin_index" value="0" />
  </group>
  <group if="$(eval location == 'rock_canyon')">
    <param name="map_origin_index" value="1" />
  </group>
  <group if="$(eval location == 'hanksville')">
    <param name="map_origin_index" value="2" />
  </group>
  <group if="$(eval location == 'gravel_pit')">
    <param name="map_origin_index" value="3" />
  </group>
  <group if="$(eval location == 'little_moab')">
    <param name="map_origin_index" value="4" />
  </group>
  

  <node pkg="swri_transform_util" exec="initialize_origin.py" name="initialize_origin" output="screen">

    <param name="local_xy_frame" value="/map"/>
    <param name="local_xy_origin" value="$(arg location)"/>

    <rosparam param="local_xy_origins">
      [{ name: byu,
        latitude: 40.2497218,
        longitude: -111.649276,
        altitude: 1376.0,
        heading: 0.0},

      { name: rock_canyon,
        latitude: 40.267147,
        longitude: -111.632455,
        altitude: 0.0,
        heading: 0.0},

      { name: hanksville,
        latitude: 38.406441,
        longitude: -110.791932,
        altitude: 1375.0,
        heading: 0.0},

      { name: gravel_pit,
        latitude: 40.322243,
        longitude: -111.644278,
        altitude: 1500.0,
        heading: 0.0},

      { name: little_moab,
        latitude: 40.057020,
        longitude: -112.012014,
        altitude: 1500.0,
        heading: 0.0}
      ]
    </rosparam>

  </node>

  <node pkg="tf" exec="static_transform_publisher" name="swri_transform" args="0 0 0 0 0 0 /map /origin 100"  />

  <node pkg="mapviz_tf" exec="rover_tf_broadcaster.py" name="rover_tf_broadcaster" output="screen"/>
  <node pkg="mapviz_tf" exec="path_to_mapviz.py" name="path_to_mapviz" output="screen"/>
  <node pkg="mapviz_tf" exec="gps_to_mapviz.py" name="gps_to_mapviz" output="screen"/>
  <node pkg="mapviz_tf" exec="click_waypoint.py" name="waypoint_picker" output="screen"/>
  <!-- Needed only for searching for services -->
  <node pkg="rosapi" exec="rosapi_node" name="rosapi"/>

</launch>