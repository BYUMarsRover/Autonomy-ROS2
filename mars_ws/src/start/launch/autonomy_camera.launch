<launch>
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" respawn="true" respawn_delay="5">
    <param name="video_device" value="/dev/rover/cameras/autonomyWebCam" />
    <param name="image_width" value="1920" />
    <param name="image_height" value="1080" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap"/>
    <param name="camera_info_url" value="file://$(find usb_cam)/head_camera.yaml"/>
    <!-- <remap from="image" to="/autonomy_cam/image_raw"/> -->
  </node>
  <node name="image_view" pkg="image_view" type="image_view" output="screen" respawn="true" respawn_delay="5">
    <remap from="image" to="/autonomy_cam/image_raw"/>
    <param name="autosize" value="true" />
  </node>
</launch>
